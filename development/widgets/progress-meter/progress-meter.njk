{% macro progress(dataset, data) %}
<div class="flex-widget-{{ dataset.display }}-{{ dataset.size }} widget-progress" data-position="{{ dataset.position }}" data-widget="{{ dataset.widget }}" data-display="{{ dataset.display }}" data-size="{{ dataset.size }}" data-rate="{{ dataset.dataRate }}" data-style="{{ dataset.style }}" data-call="{{ dataset.dataCallTo }}">
  <p class="progress-meter-heading">Sprint Progress</p>
<div id="progressMeterData" data-percent="86"></div>
<canvas id="progressMeterDisplay" class="progress-meter-canvas" width="200" height="200"></canvas>
<span>Some text here</span>
<br>
<span id="progressTime">Awaiting update...</span>
</div>
{% endmacro %}
