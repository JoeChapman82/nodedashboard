{% macro scatterChart(dataset, data) %}
<table id="dataChartScatter" class="data-table">
    {% for item in data['example-data-generator'].scatterChart %}
    <tr>
    <td>{{ item[0] }}</td>
    <td>{{ item[1] }}</td>
    </tr>
    {% endfor %}
</table>
<div class="flex-widget-{{ dataset.display }}-{{ dataset.size }} widget-scatterChart" data-position="{{ dataset.position }}" data-widget="{{ dataset.widget }}" data-display="{{ dataset.display }}" data-size="{{ dataset.size }}" data-rate="{{ dataset.dataRate }}" data-style="{{ dataset.style }}" data-call="{{ dataset.dataCallTo }}">
  <div id="scatterChartData" data-x="Large numbers" data-y="Smaller numbers" data-title="Scatter Chart"></div>
  <canvas id="scatterChartDisplay" class="scatter-chart-display" width="300" height="370"></canvas>
</div>
{% endmacro %}
